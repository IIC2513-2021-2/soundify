<h2>Users</h2>
<table>
  <thead>
    <tr>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Email</th>
      <th></th>
    </tr>
    <tbody>
      <% users.forEach((user) => { %>
        <tr>
          <td><%= user.firstName %></td>
          <td><%= user.lastName %></td>
          <td><%= user.email %></td>
          <td><a href="<%= userPath(user.id) %>">View more</a></td>
        </tr>
      <% }) %>
    </tbody>
  </thead>
</table>
<a href="<%= newUserPath %>">New user</a>
